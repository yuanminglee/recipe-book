{
    "scripts": {
        "build-css": "tailwindcss -i ./assets/css/main-source.css -o ./assets/css/main.css --minify",
        "dev-css": "tailwindcss -i ./assets/css/main-source.css -o ./assets/css/main.css --watch",
        "build": "yarn build-css && JEKYLL_ENV=production bundle exec jekyll build"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.14",
        "cssnano": "^6.0.1",
        "postcss": "^8.4.24",
        "tailwindcss": "^3.3.2"
    },
    "version": "0.0.0",
    "packageManager": "yarn@3.6.3"
}
