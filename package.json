{
  "scripts": {
    "build-css": "postcss assets/css/main.css -o assets/css/main.css",
    "watch-css": "postcss assets/css/main.css -o assets/css/main.css --watch",
    "dev": "yarn build-css && bundle exec jekyll serve",
    "build": "yarn build-css && bundle exec jekyll build"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.8",
    "postcss": "^8.4.13",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.0.24",
    "wrangler": "^3.107.2"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.6.3"
}
